{
    "name": "jworksuk/instagram-api",
    "type": "library",
    "description": "",
    "keywords": [
        "jworksuk",
        "instagram-api"
    ],
    "homepage": "https://github.com/jworksuk/instagram-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse James",
            "email": "me@jworksuk.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/collections": "^11.0",
        "nesbot/carbon": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "JWorksUK\\InstagramApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JWorksUK\\InstagramApi\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
